package date_231017;

public class Star {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		다음과 같이 출력하세요.
			    *
			   ***
			  *****
			 *******
			*********
		*/
		
		for(int i=0; i<10; i+=2) {
			for(int j=9; j>1; j-=2) {
				System.out.print(" ");
			}
			for(int k=0; k<i; k++) {
				System.out.print("*");
			}
			System.out.println("");
		}

	}

}
