package date_231019.advance_02;

public class TemporaryJob extends Employee {
	int hireYear;
	
	@Override
	public double getMonthPay() {
		double monthlyPay = pay / 12;
		return monthlyPay;
	}

	@Override
	public void showEmployeeInfo() {
		System.out.println("임시직");
		System.out.println("사번: " + eno + "이름: " + name + "급여: " + pay);
	}
}
