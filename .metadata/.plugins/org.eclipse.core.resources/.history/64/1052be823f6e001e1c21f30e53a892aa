package chap10_polymorphism.tv;

public class LgTv extends Tv {
	public void poweOn() {
		System.out.println("제조사: LG");
		// 부모클래스의 powerOn 메소드 호출
		// Tv클래스의 powerOn메소드 실행
		super.powerOn();
	}

	public void powerOff() {
		System.out.println("제조사: LG");
		super.powerOff();
	}

	public void operate(int channel) {
		System.out.println("제조사: LG");
		super.operate(channel);
	}
}
