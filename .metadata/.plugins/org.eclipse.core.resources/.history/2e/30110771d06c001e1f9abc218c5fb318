package date_231017;

public class Star {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		다음과 같이 출력하세요.
			    *
			   ***
			  *****
			 *******
			*********
		*/
		
		for(int i=0; i<5; i++) {
			for(int j=0; j>9-i-1; j++) {
				System.out.print(" ");
			}
			for(int k=0; k<i*2+1; k++) {
				System.out.print("*");
			}
			System.out.println("");
		}

	}

}
