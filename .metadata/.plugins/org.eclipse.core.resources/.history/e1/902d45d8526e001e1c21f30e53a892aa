package date_231019;

public class Mammal {
	int name;
	String kingdom;
	int divison;

	public Mammal() {

	}

	public Mammal(int name, String kingdom, int divison) {
		this.name = name;
		this.kingdom = kingdom;
		this.divison = divison;
	}

	public void eat() {
		System.out.println("새끼를 낳는다.");
	}

	public void sleep() {
		System.out.println("잠을 잔다.");
	}

	public int getName() {
		return this.name;
	}
	
	public int getKingdom() {
		return this.name;
	}
	
	public int getDivison() {
		return this.divison;
	}
}
