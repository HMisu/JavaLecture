package chap09_inherit;

public class _01_Inherit {

	public static void main(String[] args) {
		//1. 상속받은 
		HyundaiCarInherit hCar = new HyundaiCarInherit();
		
		hCar.company = "현대";
		hCar.model = "제네시스";
		hCar.color = "검정";
		hCar.price = 50000000;
		
		hCar.carInfo();
		
		//3. 자식 클래스의 고유한 기능 사용 가능
		hCar.autoPilot();
	}

}
