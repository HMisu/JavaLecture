package chap06_method.calc;

public class ReturnCalculator {
	// 1. 결과 값이 없는 메소드
	public void add() {
		System.out.println(20 + 10);
		// void로 지정된 메소드에서 return문 사용 시 에러 발생
		// return 10; 에러
	}

	// 2. 결과 값이 int형인 메소드
	public int sub() {
		return 10 + 20;
	}

	// 지정한 리턴타입과 다른 타입의 결과값이 반환되면 안됨
	public double div() {
		return (double)20/10;
	}

	public String strAdd() {
		return "hello" + "java";
	}

	public int[] createArray() {
		return new int[5];
	}
}
