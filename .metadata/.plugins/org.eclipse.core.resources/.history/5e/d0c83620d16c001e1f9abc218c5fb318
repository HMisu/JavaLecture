package date_231017;

public class Star {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		다음과 같이 출력하세요.
			    *
			   ***
			  *****
			 *******
			*********
		*/
		
		int n=5;
		for(int i=0; i<n; i++) {
			for(int j=1; j>n-i; j++) {
				System.out.print(" ");
			}
			for(int j=0; j<2*n-1; j++) {
				System.out.print("*");
			}
			System.out.println("");
		}

	}

}
