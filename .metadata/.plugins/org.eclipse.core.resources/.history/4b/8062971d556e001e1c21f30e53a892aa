package date_231019;

public class Parent {
	String lastName;
	String bloodType;
	double height;
	String skinColor;

	public Parent() {

	}

	public Parent(String lastName, String bloodType, double height, String skinColor) {
		this.lastName = lastName;
		this.bloodType = bloodType;
		this.height = height;
		this.skinColor = skinColor;
	}
}
