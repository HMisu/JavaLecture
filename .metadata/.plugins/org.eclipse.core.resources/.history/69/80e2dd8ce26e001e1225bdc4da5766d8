package date_231019.advance_02;

public class FullTime extends Employee {
	int bonus;

	@Override
	public double getMonthPay() {
		double monthlyPay = pay / 12 + bonus / 12;
		return monthlyPay;
	}

	@Override
	public void showEmployeeInfo() {
		System.out.println("정규직");
		super.showEmployeeInfo();
	}
}
